<div class="container-fluid">

	<div class="row text-center">
		<div class="col-xs-12 list-title"> List your property</div>
	</div>

	<div class="row tetx-center">

		<%=form_for :list, :html => {:multipart => true}, url: lists_path do |f| %>
			<%= f.label :home_type %><br>
			<%= f.text_field :home_type %> <br><br>

				<%= f.label :room_type %><br>
			<%= f.text_field :room_type %><br><br>

			<%= f.label :pictures %><br>
			<%= f.file_field :images, multiple: true %>
			
			<%= f.submit %>

		<%end%>
	</div>


</div>